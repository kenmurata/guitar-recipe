<h1>イベント履歴を編集する</h1>
<%= form_for(@events) do |f| %>
  <%= f.label :user_id %>
  <%= f.text_field :user_id %>
  <br>
  <%= f.label :product_id %>
  <%= f.text_field :product_id %>
  <br>
  <%= f.submit '編集' %>
<% end %>
<%= link_to '戻る' , :back, :class => 'btn btn-default' %>
<%= link_to 'TOP' , root_path, :class => 'btn btn-default' %>